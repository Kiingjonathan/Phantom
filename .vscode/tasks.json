{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc-8 build active file",
			"command": "/usr/bin/gcc-8",
			"args": [
				"-g",
				"${file}",
				"-o",
				"-Lbcm2835",
				"${fileDirname}/${fileBasenameNoExtension}"
				
			],
			"options": {
				"cwd": "/usr/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/gcc-8"
		}
	]
}